  "license": "Apache-2.0",
  "repository": "folio-org/platform-core",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folioci/"
  },
  "scripts": {
    "build": "export NODE_OPTIONS=\"--max-old-space-size=2048 $NODE_OPTIONS\"; stripes build stripes.config.js",
    "stripes": "stripes",
    "start": "stripes serve stripes.config.js",
    "build-module-descriptors": "node ./build-module-descriptors.js",
    "local": "f=stripes.config.js; test -f $f.local && f=$f.local; echo Using config $f; stripes serve $f",
    "test": "echo 'No unit tests implemented'",
    "test-int": "stripes test nightmare stripes.config.js",
    "test-regression": "stripes test nightmare stripes.config.js --run WD/checkout/users/inventory/requests/circulation/organization",
    "lint": "eslint test/ui-testing"
  },
  "dependencies": {
    "@folio/checkin": ">=1.2.100068",
    "@folio/checkout": ">=1.2.1000189",
    "@folio/circulation":">=1.2.1000116",
    "@folio/developer": ">=1.4.100025",
    "@folio/inventory": ">=1.3.1000282",
    "@folio/myprofile": ">=1.0.100024",
    "@folio/organization": ">=2.4.2000121",
    "@folio/plugin-find-instance": ">=1.0.10006",
    "@folio/plugin-find-user": ">=1.2.100030",
    "@folio/requests": ">=1.2.1000136",
    "@folio/search": ">=1.2.100099",
    "@folio/servicepoints": ">=1.0.200024",
    "@folio/stripes-components": ">=4.0.1000550",
    "@folio/stripes-core": ">=2.15.1000397",
    "@folio/stripes": ">=1.0.1",
    "@folio/tags": ">=1.0.200013",
    "@folio/users": ">=2.16.1000442"
  },
  "devDependencies": {
    "@folio/eslint-config-stripes": ">=3.2.100038",
    "eslint": "^5.0.0",
    "moment": "^2.22.2",
    "@folio/stripes-cli": ">=1.5.1000127"
  }
}
